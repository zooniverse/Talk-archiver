{
  "name": "Talk-archiver",
  "version": "0.1.0",
  "description": "Archive an Ouroboros project Talk forum, using Eleventy, webpack and PostCSS",
  "author": "Zooniverse <contact@zooniverse.org> (https://www.zooniverse.org/)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/zooniverse/Talk-archiver"
  },
  "scripts": {
    "_build": "npm-run-all clean webpack:build:assets \"_build:pages {1}\" verify > build.log --",
    "_build:pages": "npm-run-all \"build:* --quiet {1}\" \"build:users:* {1}\" \"build:subjects:* {1}\" -- ",
    "_dev": "npm-run-all --parallel \"webpack:dev:assets --watch\" \"dev:* {1}\" \"dev:users:* {1}\" \"dev:subjects:* {1}\" -- ",
    "build": "npm-run-all \"_build {1}\" copy-logs -- ",
    "build:manifest": "NODE_ENV=production node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/manifest",
    "build:boards": "NODE_ENV=production node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/boards",
    "build:tags": "NODE_ENV=production node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/tags",
    "build:api": "NODE_ENV=production node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/api",
    "build:collections": "NODE_ENV=production node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/collections",
    "build:users:index": "npm-run-all --parallel \"build:users:index:batch 1 {1}\" \"build:users:index:batch 2 {1}\" \"build:users:index:batch 3 {1}\" \"build:users:index:batch 4 {1}\" \"build:users:index:batch 5 {1}\" \"build:users:index:batch 6 {1}\" \"build:users:index:batch 7 {1}\" \"build:users:index:batch 8 {1}\" \"build:users:index:batch 9 {1}\" \"build:users:index:batch 10 {1}\" --",
    "build:users:index:batch": "NODE_ENV=production node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/users --quiet",
    "build:users:comments": "npm-run-all --parallel \"build:users:comments:batch 1 {1}\" \"build:users:comments:batch 2 {1}\" \"build:users:comments:batch 3 {1}\" \"build:users:comments:batch 4 {1}\" \"build:users:comments:batch 5 {1}\" \"build:users:comments:batch 6 {1}\" \"build:users:comments:batch 7 {1}\" \"build:users:comments:batch 8 {1}\" \"build:users:comments:batch 9 {1}\" \"build:users:comments:batch 10 {1}\" --",
    "build:users:comments:batch": "NODE_ENV=production node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/users:comments --quiet",
    "build:subjects:api": "npm-run-all --max-parallel 5 --parallel \"build:subjects:api:batch 1 {1}\" \"build:subjects:api:batch 2 {1}\" \"build:subjects:api:batch 3 {1}\" \"build:subjects:api:batch 4 {1}\" \"build:subjects:api:batch 5 {1}\" \"build:subjects:api:batch 6 {1}\" \"build:subjects:api:batch 7 {1}\" \"build:subjects:api:batch 8 {1}\" \"build:subjects:api:batch 9 {1}\" \"build:subjects:api:batch 10 {1}\" --",
    "build:subjects:api:batch": "NODE_ENV=production node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/subjects:api --quiet",
    "build:subjects:collections": "npm-run-all  --max-parallel 5 --parallel \"build:subjects:collections:batch 1 {1}\" \"build:subjects:collections:batch 2 {1}\" \"build:subjects:collections:batch 3 {1}\" \"build:subjects:collections:batch 4 {1}\" \"build:subjects:collections:batch 5 {1}\" \"build:subjects:collections:batch 6 {1}\" \"build:subjects:collections:batch 7 {1}\" \"build:subjects:collections:batch 8 {1}\" \"build:subjects:collections:batch 9 {1}\" \"build:subjects:collections:batch 10 {1}\" --",
    "build:subjects:collections:batch": "NODE_ENV=production node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/subjects:collections --quiet",
    "build:subjects:index": "npm-run-all  --max-parallel 5 --parallel \"build:subjects:index:batch 1 {1}\" \"build:subjects:index:batch 2 {1}\" \"build:subjects:index:batch 3 {1}\" \"build:subjects:index:batch 4 {1}\" \"build:subjects:index:batch 5 {1}\" \"build:subjects:index:batch 6 {1}\" \"build:subjects:index:batch 7 {1}\" \"build:subjects:index:batch 8 {1}\" \"build:subjects:index:batch 9 {1}\" \"build:subjects:index:batch 10 {1}\" --",
    "build:subjects:index:batch": "NODE_ENV=production node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/subjects --quiet",
    "clean": "rm -rf ./dist && mkdir -p ./dist/logs",
    "copy-logs": "mv build.log dist/logs/build.log",
    "deploy": "s3-batch-upload -b zooniverse-static -p ./dist -C 200 -acl 'public-read'",
    "dev": "npm-run-all clean webpack:dev:assets \"_dev {1}\" verify -- ",
    "dev:manifest": "NODE_ENV=development node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/manifest",
    "dev:api": "NODE_ENV=development node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/api --watch",
    "dev:boards": "NODE_ENV=development node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/boards --serve",
    "dev:collections": "NODE_ENV=development node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/collections --watch",
    "dev:users:index": "NODE_ENV=development node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/users --watch",
    "dev:users:comments": "NODE_ENV=development node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/users:comments --watch",
    "dev:tags": "NODE_ENV=development node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/tags --watch",
    "dev:subjects:api": "NODE_ENV=development node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/subjects:api --watch",
    "dev:subjects:collections": "NODE_ENV=development node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/subjects:collections --watch",
    "dev:subjects:index": "NODE_ENV=development node --max-old-space-size=${NODE_RAM:-12000} ./node_modules/@11ty/eleventy/cmd.js --input=src/subjects --watch",
    "verify": "node verify.js",
    "webpack:build:assets": "NODE_ENV=production webpack --mode=production",
    "webpack:dev:assets": "NODE_ENV=development webpack --mode=development"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-cache-assets": "zooniverse/eleventy-cache-assets#sha1",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@zooniverse/grommet-theme": "^2.2.0",
    "autoprefixer": "^9.7.5",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "fast-csv": "^4.1.3",
    "limited-request-queue": "^5.1.0",
    "log-symbols": "^4.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-regex": "^0.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "s3-batch-upload": "^1.3.0",
    "tailwindcss": "^1.4.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
